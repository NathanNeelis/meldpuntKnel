<header>
  <div class="headerWrapper">
    <div class="helloUser">
      <p>Hi <%=userFirstName %>!</p>
    </div>
    <div class="date">
      <div class="arrowBtn">
        <svg
          version="1.1"
          x="0px"
          y="0px"
          viewBox="0 0 13 13"
          style="enable-background: new 0 0 13 13"
          xml:space="preserve"
        >
          <path
            class="iconArrowLeft"
            d="M9.9,2.1L5.5,6.5l4.4,4.3c1.1,1.1-0.6,2.7-1.6,1.6L3.1,7.3c-0.5-0.4-0.5-1.2,0-1.6l5.2-5.2
	C9.3-0.6,11,1.1,9.9,2.1L9.9,2.1z"
          />
        </svg>
      </div>
      <p><%=currentDate %></p>
      <div class="arrowBtn">
        <svg
          version="1.1"
          x="0px"
          y="0px"
          viewBox="0 0 13 13"
          style="enable-background: new 0 0 13 13"
          xml:space="preserve"
        >
          <path
            class="iconArrowRight"
            d="M3.1,2.1l4.4,4.4l-4.4,4.3C2,12,3.7,13.6,4.7,12.5l5.2-5.2c0.5-0.4,0.5-1.2,0-1.6L4.7,0.5
        C3.7-0.6,2,1.1,3.1,2.1L3.1,2.1z"
          />
        </svg>
      </div>
    </div>
    <div class="userImage">
      <a href="#profile">
        <img
          src="images/mountains.jpg"
          alt="background user icon"
          class="userImageBg"
        />
        <p class="initials"><%=userInitials %></p>
      </a>
    </div>
  </div>

  <%- include('../atoms/navigatie.ejs'); %>
</header>
